{
  "name": "the-plague",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:local": "cross-env NODE_ENV=development ENV_FILE=.env.local docker-compose up -d --build && cross-env ENV_FILE=.env.local next build",
    "build:production": "cross-env NODE_ENV=production ENV_FILE=.env.production docker-compose up -d --build && cross-env ENV_FILE=.env.production next build",
    "start:local": "cross-env NODE_ENV=development ENV_FILE=.env.local next start -p 3001",
    "start:production": "cross-env NODE_ENV=production ENV_FILE=.env.production next start -p 3000"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.685.0",
    "@aws-sdk/s3-request-presigner": "^3.685.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "ag-grid-community": "^32.3.3",
    "ag-grid-react": "^32.3.3",
    "chart.js": "^4.4.6",
    "flowbite": "^2.5.2",
    "flowbite-react": "^0.10.2",
    "js-cookie": "^3.0.5",
    "next": "14.2.15",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "sharp": "^0.33.5",
    "swiper": "^11.1.14",
    "tailwind-scrollbar": "^3.1.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5"
  }
}
